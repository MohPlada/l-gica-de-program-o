algoritmo "ex011"

var

a, b, c, delta, x1, x2 :real

inicio

Escreva (" Digite o valor de a: ")
Leia (a)
Escreva ("Digite o valor de b: ")
leia (b)
Escreva ("Digite o valor de c: ")
Leia (c)
delta <- b^b-4*a*c
x1 <- (-b + delta)/(2*a)
x2 <- (-b - delta)/(2*a)
EscrevaL (delta)



fimalgoritmo
